{
  "name": "ecommerce-analytics-dashboard-backend",
  "version": "0.0.1",
  "description": "ecommerce analytics dashboard backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t ecommerce-analytics-dashboard-backend .",
    "docker:run": "docker run -p 3000:3000 -d ecommerce-analytics-dashboard-backend",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Carly Sandler <carly.sandler@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^6.1.3",
    "@loopback/core": "^5.1.3",
    "@loopback/repository": "^6.1.3",
    "@loopback/rest": "^13.1.3",
    "@loopback/rest-explorer": "^6.1.3",
    "@loopback/service-proxy": "^6.1.3",
    "loopback-connector-mysql": "^5.3.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.3",
    "@loopback/eslint-config": "^14.0.4",
    "@loopback/testlab": "^6.1.3",
    "@types/express-session": "^1.17.8",
    "@types/lodash": "^4.14.199",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^16.18.50",
    "@types/node-fetch": "^2.6.6",
    "@types/oauth": "^0.9.2",
    "@types/passport": "^1.0.13",
    "@types/passport-google-oauth2": "^0.1.6",
    "@types/passport-google-oauth20": "^2.0.12",
    "@types/passport-oauth2": "^1.4.13",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
